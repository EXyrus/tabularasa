{
  "name": "@tabula-rasa/website",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "homepage": "https://tabularasa.ng",
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "cache:clear": "npx rimraf node_modules/.cache",
    "code:check:ci": "npm-run-all -p --aggregate-output tsc code:check",
    "code:check": "npm run eslint -- --cache --max-warnings=0 && npm run prettier:check",
    "code:fix": "npm run eslint:fix && npm run prettier:fix",
    "cypress:open:ci": "npm run cypress:open -- --config-file=cypress.ci.config.ts",
    "cypress:open": "npm run cypress -- open",
    "cypress:run:ci": "npm run cypress:run -- --config-file=cypress.ci.config.ts",
    "cypress:run:int": "npm run cypress:run -- --config-file=cypress.int.config.ts",
    "cypress:run": "npm run cypress -- run --browser=chrome",
    "cypress": "npx cypress",
    "depcruise:check": "npm run depcruise -- --config .dependency-cruiser.cjs src",
    "depcruise:graph": "npx depcruise --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "depcruise": "npx depcruise",
    "eslint:cache:clear": "npx rimraf node_modules/.cache/eslint-loader",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint \"src/**/*.{ts?(x),js?(x),m(js|ts)}\" \"cypress/**/*.{ts?(x),js?(x),m(js|ts)}\"",
    "prebuild:ci": "npm-run-all -p --aggregate-output env code:check:ci",
    "prebuild": "tsc",
    "prepare": "husky",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "prettier": "prettier \"(src|public|cypress)/**/*.{ts?(x),js?(x),m(js|ts),json,less,md,htm?(l),php}\"",
    "preview": "vite preview",
    "serve-json": "json-server --watch data.json --port 4000",
    "start": "rimraf dev-dist && vite",
    "tsc:check": "npm run tsc -- --skipLibCheck false",
    "tsc": "npx tsc",
    "type-coverage:detail": "npm run type-coverage -- --detail",
    "type-coverage:update": "npm run type-coverage -- --update-if-higher",
    "type-coverage": "npx type-coverage"
  },
